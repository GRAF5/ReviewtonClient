{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": ["error", "always"],
        "indent": ["error", 2],
        "quotes": ["error", "single"],
        "curly": ["error"],
        "eqeqeq": ["error"],
        "new-cap": ["error", {"properties": false}],
        "no-empty": ["error"],
        "no-new": ["error"],
        "no-plusplus": ["off"],
        "max-depth": ["error", 5],
        "complexity": ["error", 8],
        "max-len": ["error", 120],
        "no-var": ["error"],
        "no-eq-null": ["error"],
        "valid-typeof": ["error"],
        "no-shadow": ["error"],
        "no-new-func": ["error"],
        "no-unused-vars": ["off"],
        "keyword-spacing": ["error"],
        "space-infix-ops": ["error"],
        "operator-linebreak": ["error", "after"],
        "no-mixed-spaces-and-tabs": ["error"],
        "space-unary-ops": ["error", {"nonwords": false, "overrides": {}}],
        "space-before-blocks": ["error", "always"],
        "eol-last": ["error"],
        "array-bracket-spacing": ["error", "never"],
        "space-in-parens": ["error", "never"],
        "no-multiple-empty-lines": ["error"],
        "comma-dangle": ["error", "never"]
    }
}
